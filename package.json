{
    "name": "amazonka",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "test-unit": "jest --testPathPattern='/__tests__/unit/.*'",
        "test-graph": "jest --testPathPattern='/__tests__/qraphql_queries/.*'",
        "test-func": "jest --runInBand --testPathPattern='/__tests__/functional/.*'",
        "test": "npm run test-unit && npm run test-graph && npm run test-func",
        "test-front": "cd front && npm test",
        "test-all": "npm run test && npm run test-front",
        "start:dev": "npx nodemon server.js",
        "start": "node server",
        "prepare": "npx husky install",
        "pre-commit": "pretty-quick --staged",
        "postinstall": "cd front && npm install && cd .. && npx basetag link"
    },
    "jest": {
        "transform": {
            "^.+\\.jsx?$": "babel-jest"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Darkosz1012/amazonka.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Darkosz1012/amazonka/issues"
    },
    "homepage": "https://github.com/Darkosz1012/amazonka#readme",
    "dependencies": {
        "@babel/runtime": "^7.14.0",
        "@graphql-tools/schema": "^7.1.3",
        "apollo-server": "^2.23.0",
        "async": "^3.2.0",
        "crypto": "^1.0.1",
        "dataloader": "^2.0.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "fs": "0.0.1-security",
        "graphql": "^15.5.0",
        "graphql-query-to-json": "^1.0.2",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.12.5",
        "path": "^0.12.7"
    },
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.13.15",
        "@babel/preset-env": "^7.14.1",
        "@babel/preset-react": "^7.13.13",
        "@commitlint/cli": "^12.1.1",
        "@commitlint/config-conventional": "^12.1.1",
        "babel-jest": "26.6.0",
        "basetag": "^2.0.1",
        "easygraphql-tester": "^6.0.1",
        "husky": "^6.0.0",
        "jest": "26.6.0",
        "mockingoose": "^2.15.2",
        "mongodb-memory-server": "^6.9.6",
        "nodemon": "^2.0.7",
        "prettier": "^2.3.0",
        "pretty-quick": "^3.1.0",
        "rimraf": "^3.0.2",
        "supertest": "^6.1.3"
    }
}
